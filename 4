#include<iostream>
#include<vector>
#include<algorithm>
using namespace std; 
bool ispalindrome(long int n)
{
    vector<int> v;
    while(n>0)
    {
        int rem=n%10;
        v.push_back(rem);
        n/=10;
    }
    vector<int>::iterator it=v.begin();
    vector<int>::iterator it1=v.end();
    it1--;
    while(it<=it1)
    {
        
        if(*it!=*it1)
            return false;
        it++;
        it1--;
    }

    return true;
}
long long int max(long long int a, long long int b)
{
    if(a>b)
        return a;
    return b;
}
int main()
{   
    long long temp=0;
    int a=999;
    int b=999;
    int i=0;
    long int check=1;
    for(;a>=100;a--)
    {
        for(b=999;b>=100;b--)
        {
            check=a*b;
            if(ispalindrome(check))
            {
                temp=max(temp,check);
            }
        }
    }
    cout<<temp;
    return 0;
}
