#include<iostream>
#include<algorithm>
using namespace std;
long long int generate(char* A, int i)
{
    
    long int m=1;
    int j=i+13;
    for(;i<j;i++)
    {
        m*=(A[i]-'0');
    }
    
    return m;
}
int main()
{
    cout<<"Enter the number of digits-> ";
    int n;
    cin>>n;
    char A[n];
    for(int i=n-1;i>=0;)
    {
        char ch=cin.get();
        if(ch!='\n')
            A[i--]=ch;
    }
    for(int i=n-1;i>=0;i--)
        cout<<A[i];
    cout<<endl;
    
    long long int m=1;
    long long int answer=0;
    for(int i=0;i<=(n-13);i++)
    {
        
       m=generate(A,i);
       answer=max(m,answer);
    }
    cout<<"answer is "<<answer;
    return 0;
}
